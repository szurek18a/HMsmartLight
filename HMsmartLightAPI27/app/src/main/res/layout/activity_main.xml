<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F72F3033"
    android:foregroundTint="#FFFFFF"
    tools:context=".MainActivity">

    <ListView
        android:id="@+id/listaZarowek"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="9dp"
        android:dividerHeight="3.0sp"
        android:divider="#673AB7"
        app:layout_constraintBottom_toTopOf="@+id/dodajActionButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/dodajActionButton"
        android:layout_width="57dp"
        android:layout_height="61dp"
        android:layout_marginStart="334dp"
        android:layout_marginTop="662dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="32dp"
        android:background="#FFFFFF"
        android:clickable="true"
        android:tint="@color/white"
        app:backgroundTint="#673AB7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:maxImageSize="32dp"
        app:srcCompat="@android:drawable/ic_input_add"
        app:useCompatPadding="false"
        tools:ignore="SpeakableTextPresentCheck"
        android:contentDescription="TODO" />

</android.support.constraint.ConstraintLayout>